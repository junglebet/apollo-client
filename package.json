    "crypto-hash": "^2.0.1",
    "defu": "^6.1.3",
    "destr": "^1.2.2",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^5.14.2",
    "jiti": "^1.21.0",
    "ohash": "^1.1.3",
    "pusher-js": "^8.3.0"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/schema": "^3.8.2",
    "@nuxt/ui": "^0.4.1",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@types/node": "^18.18.13",
    "eslint": "^8.54.0",
    "nuxt": "^3.8.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "resolutions": {
    "@nuxtjs/apollo": "link:."
  },
  "packageManager": "pnpm@7.29.1"
